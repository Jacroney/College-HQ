# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_USER_POOL_ID=your_user_pool_id
VITE_USER_POOL_CLIENT_ID=your_user_pool_client_id
VITE_OAUTH_DOMAIN=your_oauth_domain
VITE_OAUTH_REDIRECT_SIGN_IN=http://localhost:5173/
VITE_OAUTH_REDIRECT_SIGN_OUT=http://localhost:5173/

# API Gateway Configuration (Single endpoint for all services)
VITE_API_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/dev

# Individual Lambda Function URLs (for direct invocation if needed)
VITE_AUTH_HANDLER_URL=your_auth_handler_function_url
VITE_PROFILE_SERVICE_URL=your_profile_service_function_url
VITE_ADVISING_SERVICE_URL=your_advising_service_function_url
VITE_COURSE_SERVICE_URL=your_course_service_function_url
VITE_CONVERSATION_SERVICE_URL=your_conversation_service_function_url

# Lambda Function Names (for development/testing)
VITE_AUTH_HANDLER_NAME=auth-handler
VITE_PROFILE_SERVICE_NAME=profile-service
VITE_ADVISING_SERVICE_NAME=advising-service
VITE_COURSE_SERVICE_NAME=course-service
VITE_CONVERSATION_SERVICE_NAME=conversation-service

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_SERVICE_MONITORING=false

# Development
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_API_MOCKING=false